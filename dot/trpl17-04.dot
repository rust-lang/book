digraph {
   rankdir = RL;
   overlap = false;
   dpi = 300.0;
   splines = false;
   cluster = true;
   
   node [shape = "plaintext";];
   
   fut1 [label = <<table border="0" cellborder="1" cellspacing="0">
      <tr><td sides="B">fut1</td></tr>
      <tr><td>0</td></tr>
      <tr><td port="target">1</td></tr>
      <tr><td port="source">  </td></tr>
      </table>>;];
   
   edge [tailclip = "false";];
   fut1:source:c -> fut1:target [dir = forward;];
}